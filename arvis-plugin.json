{
    "$schema": "https://raw.githubusercontent.com/jopemachine/arvis-extension-validator/master/plugin-schema.json",
    "creator": "jopemachine",
    "name": "arvis-terminal-plugin",
    "enabled": true,
    "description": "Terminal open plugin for Arvis",
    "platform": [
        "darwin",
        "linux"
    ],
    "readme": "",
    "version": "0.1.3",
    "webAddress": "https://github.com/arvis-plugins/arvis-terminal-plugin",
    "main": "index.js",
    "defaultIcon": "icon.png",
    "variables": {
        "prefix": ">",
        "logCnt": 10
    },
    "actions": [
        {
            "type": "script",
            "modifiers": "normal",
            "script": "node openTerminal.js '{query}'"
        }
    ]
}
